{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.cursorBlinking": "solid",
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.lineHeight": 18,
    "editor.fontSize": 14,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },
    "editor.suggest.localityBonus": true,
    "editor.fontFamily": "'JetBrains Mono NL', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.wordWrap": "on",
    "editor.scrollBeyondLastLine": false,
    "editor.rulers": [80],
    "editor.cursorSurroundingLines": 2,
    "editor.find.autoFindInSelection": "multiline",
    "editor.accessibilitySupport": "off",
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Path": "/home/tomek/.vscode/python/bin/autopep8",
    "python.linting.flake8Path": "/home/tomek/.vscode/python/bin/flake8",
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.enabled": true,
    "python.showStartPage": false,
    "typescript.disableAutomaticTypeAcquisition": true,
    "window.titleBarStyle": "custom",
    "window.newWindowDimensions": "maximized",
    "window.zoomLevel": 0,
    "workbench.editor.showTabs": false,
    "workbench.enableExperiments": false,
    "workbench.editor.revealIfOpen": true,
    "workbench.activityBar.visible": false,
    "workbench.editor.enablePreview": false,
    "workbench.colorCustomizations": {
        "[Default Dark+]": {
            "editor.foreground": "#eeeeee",
            "editor.background": "#282828",
            "editor.lineHighlightBackground": "#1e1e1e"
        },
        "[Default Light+]": {
            "editor.foreground": "#282828",
            "editor.background": "#eeeeee",
            "editor.lineHighlightBackground": "#e1e1e1"
        }
    },
    "workbench.startupEditor": "none",
    "files.trimTrailingWhitespace": true,
    "extensions.ignoreRecommendations": true,
    "files.watcherExclude": {
        "**/venv/**": true
    },
    "search.exclude": {
        "**/venv/**": true
    },
}
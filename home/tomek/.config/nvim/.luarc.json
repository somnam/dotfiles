{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT"
  },
  "semantic": {
    "enable": false
  },
  "workspace": {
    "library": [
      "lua",
      "$VIMRUNTIME"
    ],
    "maxPreload": 1000,
    "checkThirdParty": false
  },
  "completion": {
    "showWord": "Disable"
  },
  "diagnostics": {
    "unusedLocalExclude": [ "_*" ]
  },
  "telemetry.enable": false
}
